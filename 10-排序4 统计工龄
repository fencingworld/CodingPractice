#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int n;
    vector<int >s(51,0);
    cin>>n;
    for (int i=0;i<n;i++)
    {
        int t;
        cin>>t;
        s[t]++;
    }
    for (int i=0;i<51;i++)
    {
        if (0!=s[i])
        {
            cout<<i<<":"<<s[i]<<endl;
        }
    }
    //cout << "Hello world!" << endl;
    return 0;
}
